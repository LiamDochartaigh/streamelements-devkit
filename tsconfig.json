{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.node.json"
    },
    {
      "path": "./tsconfig.app.json"
    }
  ],
  "compilerOptions": {
    "allowJs": true,
    "lib": ["es6", "dom", "es2016"],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "outDir": "./dist",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": [
      "vite/client",
    ],
    "strict": true,
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.js",
    "src/**/*.json",
  ],
}